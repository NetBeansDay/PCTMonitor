<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Layers xmlns="http://uk.co.moons.pct/schema/Layers" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://uk.co.moons.pct/schema/Layers ../Layers.xsd">
    <Type>Model</Type>
    <Layer>
        <Name>Interface</Name>
        <Controller>
            <Name>Atom</Name>
            <Functions>
                <InputFunctions>
                    <Input>
                        <Name>AtomInput</Name>
                        <Description>...</Description>
                        <NeuralFunction>
                            <Type>SineGenerator</Type>
                            <Parameters>
                                <Name>Amplitude</Name>
                                <Value>1</Value>
                                <DataType>Double</DataType>
                            </Parameters>
                            <Parameters>
                                <Name>Frequency</Name>
                                <Value>1</Value>
                                <DataType>Double</DataType>
                            </Parameters>
                            <Parameters>
                                <Name>Phase</Name>
                                <Value>0</Value>
                                <DataType>Double</DataType>
                            </Parameters>
                            <Parameters>
                                <Name>XShift</Name>
                                <Value>0</Value>
                                <DataType>Double</DataType>
                            </Parameters>
                            <Parameters>
                                <Name>YShift</Name>
                                <Value>0</Value>
                                <DataType>Double</DataType>
                            </Parameters>
                            <Parameters>
                                <Name>Step</Name>
                                <Value>1</Value>
                                <DataType>Double</DataType>
                            </Parameters>
                            <Links>
                                <Index>0</Index>
                                <Name>Pause</Name>
                            </Links>
                        </NeuralFunction>
                    </Input>
                    <Transfers>
                        <Transfer>
                            <Name>Pause</Name>
                            <Description>...</Description>
                            <NeuralFunction>
                                <Type>Pause</Type>
                                <Parameters>
                                    <Name>ConstantRate</Name>
                                    <Value>true</Value>
                                    <DataType>Boolean</DataType>
                                </Parameters>
                                <Parameters>
                                    <Name>Pause</Name>
                                    <Value>50</Value>
                                    <DataType>Double</DataType>
                                </Parameters>
                            </NeuralFunction>
                        </Transfer>
                    </Transfers>
                </InputFunctions>
                <ReferenceFunctions>
                    <Reference>
                        <Name>AtomReference</Name>
                        <Description>...</Description>
                        <NeuralFunction>
                            <Type>Constant</Type>
                            <Parameters>
                                <Name>Constant</Name>
                                <Value>20</Value>
                                <DataType>Double</DataType>
                            </Parameters>
                        </NeuralFunction>
                    </Reference>
                </ReferenceFunctions>
                <ErrorFunctions>
                    <Error>
                        <Name>AtomError</Name>
                        <Description>...</Description>
                        <NeuralFunction>
                            <Type>Subtract</Type>
                            <Links>
                                <Index>0</Index>
                                <Name>AtomReference</Name>
                            </Links>
                            <Links>
                                <Index>1</Index>
                                <Name>AtomInput</Name>
                            </Links>
                        </NeuralFunction>
                    </Error>
                </ErrorFunctions>
                <OutputFunctions>
                    <Output>
                        <Name>AtomOutput</Name>
                        <Description>...</Description>
                        <NeuralFunction>
                            <Type>Constant</Type>
                            <Parameters>
                                <Name>Constant</Name>
                                <Value>20</Value>
                                <DataType>Double</DataType>
                            </Parameters>
                            <Links>
                                <Index>0</Index>
                                <Name>AtomReference</Name>
                            </Links>
                            <Links>
                                <Index>1</Index>
                                <Name>AtomError</Name>
                            </Links>
                        </NeuralFunction>
                    </Output>
                    <Transfers>
                        <Transfer>
                            <Name>TwoConstant</Name>
                            <Description>...</Description>
                            <NeuralFunction>
                                <Type>Constant</Type>
                                <Parameters>
                                    <Name>Constant</Name>
                                    <Value>20</Value>
                                    <DataType>Double</DataType>
                                </Parameters>
                                <Links>
                                    <Index>0</Index>
                                    <Name>AtomOutput</Name>
                                </Links>
                                <Links>
                                    <Index>1</Index>
                                    <Name>Pause</Name>
                                </Links>
                            </NeuralFunction>
                        </Transfer>
                    </Transfers>
                </OutputFunctions>
            </Functions>
        </Controller>
    </Layer>
</Layers>
